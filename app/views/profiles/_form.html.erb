<%= simple_form_for(@profile) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">

    <%= f.input :user_id %>

    <%= f.input :birthday %>

    <%= f.input :location %>

    <%= f.input :facebook %>

    <%= f.input :twitter %>

    <%= f.input :about %>

  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
